# -*- mode: snippet -*-
# name: Org bold
# type: command
# binding: s-b
# --
(yasp-selected-current)
(yas-expand-snippet
  (if (region-active-p)
    (if (texmathp)
      "\\mathbf{`yas-selected-text`}$0"
     "**`yas-selected-text`**$0"
    )
   (if (texmathp)
      "\\mathbf{$0}"
     "**$0**"
    )
  )
)
